{
    "name": "luoxiaojun/sw-fw-less",
    "description": "Swoole Http Server without framework",
    "type": "library",
    "keywords": [
        "swoole",
        "framework",
        "coroutine",
        "php"
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-swoole": ">=4.4.0",
        "aura/sqlquery": "^2",
        "bt51/ntp": "~0.1.0",
        "cakephp/event": "3.7.1",
        "cakephp/validation": "~3",
        "dragonmantank/cron-expression": "^2.3",
        "filp/whoops": "~2.1@stable",
        "huang-yi/swoole-watcher": "~0.0",
        "koala-framework/file-watcher": " ~1.0",
        "league/flysystem": "~1",
        "luoxiaojun/closure-php": "dev-main",
        "luoxiaojun/grpc": "dev-master",
        "luoxiaojun1992/monolog-co-stream-handler": "~1.0",
        "monolog/monolog": "^1",
        "nesbot/carbon": "^1.26.3",
        "nikic/fast-route": "^1",
        "opis/closure": "^3.3",
        "php-di/php-di": "~6.0",
        "ramsey/uuid": "~3",
        "swlib/saber": "^1",
        "symfony/console": "~3.3.0",
        "symfony/translation": "~4.0.0",
        "symfony/var-dumper": "~3.0",
        "vlucas/phpdotenv": "~2.2"
    },
    "suggest": {
        "ext-pdo": "*",
        "ext-phalcon": "*",
        "ext-jsonnet": "*",
        "ext-redis": "*",
        "ext-curl": "*",
        "luoxiaojun1992/laravel-elasticsearch": "~1.0",
        "elasticsearch/elasticsearch": "~6",
        "apache/thrift": "0.11.0",
        "lcobucci/jwt": "~3.2",
        "openzipkin/zipkin": "1.3.2",
        "overtrue/flysystem-qiniu": "~1",
        "php-amqplib/php-amqplib": "2.8.1",
        "xxtime/flysystem-aliyun-oss": "1.2.0",
        "zendframework/zend-diactoros": "~2.1"
    },
    "license": "apache-2.0",
    "authors": [
        {
            "name": "luoxiaojun",
            "email": "luoxiaojun1992@sina.cn"
        }
    ],
    "autoload": {
        "psr-4": {"SwFwLess\\": "src/"},
        "files": ["src/components/functions.php", "dist/closure/meta.php"]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "mockery/mockery": "~1.2",
        "phpunit/phpunit": "~7.5"
    }
}
